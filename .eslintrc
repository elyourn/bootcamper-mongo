{
  "extends": ["airbnb-base"],
  "plugins": ["prettier"],
  "rules": {
    "space-before-function-paren":  0,
    "indent": ["error", 4],
    "no-underscore-dangle": 0,
    "function-paren-newline": 0,
    "arrow-parens": ["error", "always"],
    "require-yield": 0,
    "implicit-arrow-linebreak": 0,
    "object-curly-newline": 0
  },
  "env": {
    "es6": true,
    "node": true
  }
}